{
    "name": "bbuehrle/insecure-mailer",
    "description": "Symfony Insecure Mailer Bridge",
    "type": "symfony-mailer-bridge",
    "require": {
        "php": ">=7.2.5",
        "psr/event-dispatcher": "^1",
        "symfony/mailer": "^4.4|^5.4|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.x-dev"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Symfony\\Component\\Mailer\\Bridge\\InsecureMailer\\": "src/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": { "Symfony\\Component\\Mailer\\Bridge\\InsecureMailer\\Tests\\": "tests/" }
    },
    "authors": [
        {
            "name": "Brian Buehrle",
            "email": "brian@buehrle.us"
        }
    ],
    "minimum-stability": "dev"
}
